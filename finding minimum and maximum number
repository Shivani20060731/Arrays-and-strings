#include <iostream>
using namespace std;
int main() {
    int arr[] = {7, 9, 6, 4, 0};
    int min = arr[0], max = arr[0];
   
    for(int i = 1; i < 5; i++) {
        if(arr[i] < min) min = arr[i];
        if(arr[i] > max) max = arr[i];
    }
    cout << "min=" << min << ", max=" << max << endl;  
    return 0;
}


/output
min=0, max=9
